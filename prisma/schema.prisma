generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id       Int       @id @default(autoincrement())
  name     String
  trackers Tracker[]
}

model Tracker {
  id        Int     @id @default(autoincrement())
  url       String
  element   String
  product   Product @relation(fields: [productId], references: [id])
  productId Int
}
